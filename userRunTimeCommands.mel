runTimeCommand
	-annotation "shapeparent"
	-category "User"
	-commandLanguage "mel"
	-command ("{\nstring $allObjs[] = `ls -sl`;\nstring $FatherObj[] = `ls -sl -tail 1`;\nint $maxNum = `size $allObjs`;\nint $i;\nfor ($i=0;$i<$maxNum - 1;$i++)\n{\nstring $shapes[] = `listRelatives -s -path $allObjs[$i]`;\nparent -r -s $shapes $FatherObj[0];\ndelete $allObjs[$i];\n}\n}\n\n")
	shapeparent;

