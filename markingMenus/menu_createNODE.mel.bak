
    menuItem
        -label "CreateNode" 
        -divider 0
        -subMenu 0
        -tearOff 0
        -command "{\nstring $NodeWindow = \"PxzNodeWindow\";\nif(`window -ex $NodeWindow`)deleteUI -wnd $NodeWindow;\nif(`windowPref -ex $NodeWindow`) windowPref -r $NodeWindow;\nwindow -t \"Create Node Window\" -w 312 -h 210 $NodeWindow;\nstring $MYformLayout = `formLayout`;\nstring $textlabel = `text -l \"Node name\" -w 80 -h 20 textlabel`;\nstring $nameText = `textField  -w 200 -h 20 -bgc 0.894 0.736 0.791 nameText1`;\nstring $buttonA1 = `button -l \"测量\" -w 70 -h 30 -c (\"CL\") buttonA1`;\nstring $buttonA2 = `button -l \"条件\" -w 70 -h 30 -c (\"TJ\") buttonA2`;\nstring $buttonA3 = `button -l \"乘除\" -w 70 -h 30 -c (\"CC\") buttonA3`;\nstring $buttonA4 = `button -l \"加减\" -w 70 -h 30 -c (\"JJ\") buttonA4`;\nstring $buttonB1 = `button -l \"反向\" -w 70 -h 30 -c (\"FX\") buttonB1`;\nstring $buttonB2 = `button -l \"毛囊\" -w 70 -h 30 -c (\"MN\") buttonB2`;\nstring $buttonB3 = `button -l \"曲面采样\" -w 70 -h 30 -c (\"QMCY\") buttonB3`;\nstring $buttonB4 = `button -l \"曲线采样\" -w 70 -h 30 -c (\"QXCY\") buttonB4`;\nstring $buttonC1 = `button -l \"颜色融合\" -w 70 -h 30 -c (\"YSRH\") buttonC1`;\nstring $buttonC2 = `button -l \"图层\" -w 70 -h 30 -c (\"TC\") buttonC2`;\nstring $buttonC3 = `button -l \"setRange\" -w 70 -h 30 -c (\"SET\") buttonC3`;\nstring $buttonC4 = `button -l \"Clamp\" -w 70 -h 30 -c (\"CLM\") buttonC4`;\nstring $buttonD1 = `button -l \"合并曲线\" -w 148 -h 30 -c (\"HBQX\") buttonD1`;\nstring $buttonD2 = `button -l \"移动物体\" -w 148 -h 30 -c (\"YDWT\") buttonD2`;\nstring $Email    = `text -l \"E_mail : zclongpop123@yahoo.cn\" -w 300 -h 20 email`;\nformLayout -edit\n-attachForm     $textlabel \"top\"   4 \n-attachForm     $textlabel \"left\"   4 \n\n-attachForm     $nameText \"top\"   4 \n-attachControl  $nameText \"left\"  10  $textlabel\n\n-attachControl  $buttonA1 \"top\"   8   $textlabel\n-attachForm     $buttonA1 \"left\"   4 \n\n-attachControl  $buttonA2 \"top\"   8    $textlabel\n-attachControl  $buttonA2 \"left\"   8   $buttonA1\n\n-attachControl  $buttonA3 \"top\"   8    $textlabel\n-attachControl  $buttonA3 \"left\"   8   $buttonA2\n\n-attachControl  $buttonA4 \"top\"   8    $textlabel\n-attachControl  $buttonA4 \"left\"   8   $buttonA3\n\n-attachControl  $buttonB1 \"top\"   10   $buttonA1\n-attachForm     $buttonB1 \"left\"   4   \n\n-attachControl  $buttonB2 \"top\"   10   $buttonA1\n-attachControl  $buttonB2 \"left\"   8   $buttonB1\n\n-attachControl  $buttonB3 \"top\"   10   $buttonA1\n-attachControl  $buttonB3 \"left\"   8   $buttonB2\n\n-attachControl  $buttonB4 \"top\"   10   $buttonA1\n-attachControl  $buttonB4 \"left\"   8   $buttonB3\n\n-attachControl  $buttonC1 \"top\"   10   $buttonB1\n-attachForm     $buttonC1 \"left\"   4   \n\n-attachControl  $buttonC2 \"top\"   10   $buttonB1\n-attachControl  $buttonC2 \"left\"   8   $buttonC1\n\n-attachControl  $buttonC3 \"top\"   10   $buttonB1\n-attachControl  $buttonC3 \"left\"   8   $buttonC2\n\n-attachControl  $buttonC4 \"top\"   10   $buttonB1\n-attachControl  $buttonC4 \"left\"   8   $buttonC3\n\n-attachControl  $buttonD1 \"top\"   8   $buttonC1\n-attachForm     $buttonD1 \"left\"   4\n\n-attachControl  $buttonD2 \"top\"   10   $buttonC1\n-attachControl  $buttonD2 \"left\"   8   $buttonD1\n \n-attachControl  $Email    \"top\"   5   $buttonD1 \n-attachForm     $Email    \"left\"   6\n\n$MYformLayout;\nshowWindow;\n//测量\nproc CL()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  distanceBetween;}\nelse\n{createNode -n ($name) distanceBetween;}\n}\n\n//条件\nproc TJ()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  condition;}\nelse\n{createNode -n ($name) condition;}\n}\n//乘除\nproc CC()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  multiplyDivide;}\nelse\n{createNode -n ($name) multiplyDivide;}\n}\n//加减\nproc JJ()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  plusMinusAverage;}\nelse\n{createNode -n ($name) plusMinusAverage;}\n}\n//反向\nproc FX()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  reverse;}\nelse\n{createNode -n ($name) reverse;}\n}\n//毛囊\nproc MN()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  follicle;}\nelse\n{createNode -n ($name) follicle;}\n}\n//曲面采样\nproc QMCY()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  pointOnSurfaceInfo;}\nelse\n{createNode -n ($name) pointOnSurfaceInfo;}\n}\n//曲线采样\nproc QXCY()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  curveInfo;}\nelse\n{createNode -n ($name) curveInfo;}\n}\n//颜色融合\nproc YSRH()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  blendColors;}\nelse\n{createNode -n ($name) blendColors;}\n}\n//图层\nproc TC()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  displayLayer;}\nelse\n{createNode -n ($name) displayLayer;}\n}\n//Set Range\nproc SET()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode  setRange;}\nelse\n{createNode -n ($name) setRange;}\n}\n\n//clamp\nproc CLM()\n{\n$name = `textField -q -tx nameText1`;\nif(size($name) == 0)\n{createNode clamp;}\nelse\n{createNode -n ($name) clamp;}\n}\n//合并曲线\nproc HBQX()\n{\nstring $sel[] = `ls -sl`;\nstring $first = $sel[0];\nstring $second = $sel[1];\n\nstring $firstshape[] = `listRelatives -s -path $first`;\nparent -r -s $firstshape $second;\nselect -r $first;\ndoDelete;\nselect -r $second;\n//xform -cp;\n}\n//移动物体\nproc YDWT()\n{\nstring $currentSel[] = `ls -sl`;\nstring $base = $currentSel[0];\nstring $target = $currentSel[1];\n\t\nfloat $worldSpaceOfBase[] = `xform -query -worldSpace -rotatePivot $base`;\nmove -absolute $worldSpaceOfBase[0] $worldSpaceOfBase[1] $worldSpaceOfBase[2] $target;\n\n}\n\n}" 
        -altModifier 0
        -optionModifier 0
        -commandModifier 0
        -ctrlModifier 0
        -shiftModifier 0
        -optionBox 0
        -enable 1
        -data 0
        -radialPosition "E" 
        -allowOptionBoxes 1
        -postMenuCommandOnce 0
        -enableCommandRepeat 1
        -image "CreateNode" 
        -echoCommand 0
        -annotation "CreateNode" 
        -italicized 0
        -boldFont 1
        menuEditorMenuItem1;

setParent -m ..;

